{
  "openapi": "3.1.0",
  "info": {
    "title": "Model Control Interface",
    "description": "Control and monitor executable units with linting and persistence options.",
    "version": "1.0.0",
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/license/mit"
    }
  },
  "servers": [
    {
      "url": "http://petstore.swagger.io/v2",
      "description": "Production server"
    }
  ],
  "tags": [
    {
      "name": "process",
      "description": "Management of executable units"
    },
    {
      "name": "definition",
      "description": "Management of API definitions"
    },
    {
      "name": "module",
      "description": "Management of WASM modules"
    }
  ],
  "components": {
    "securitySchemes": {
      "mci_auth": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "http://petstore.swagger.io/oauth/dialog",
            "scopes": {
              "write:processes": "modify processes",
              "read:processes": "read processes",
              "write:definitions": "modify definitions",
              "read:definitions": "read definitions",
              "write:modules": "modify modules",
              "read:modules": "read modules"
            }
          }
        }
      },
      "clientId": {
        "type": "apiKey",
        "in": "header",
        "name": "X-Client-ID",
        "description": "Client ID to distinguish between different client applications"
      },
      "apiToken": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-Token",
        "description": "API token for service-tqueriesnao-service authentication"
      }
    },
    "schemas": {
      "Process": {
        "type": "object",
        "required": ["code", "environment"],
        "properties": {
          "pid": {
            "type": "integer",
            "format": "int64",
            "readOnly": true,
            "examples": [101]
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "readOnly": true,
            "enum": ["queued", "running", "complete"],
            "description": "The current state of the process."
          },
          "exit_state": {
            "type": "string",
            "readOnly": true,
            "enum": ["success", "failed", "terminated"],
            "description": "The result of the last execution."
          },
          "code": {
            "type": "string"
          },
          "environment": {
            "type": "string"
          },
          "extras": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            }
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "readOnly": true,
            "description": "Timestamp when the process was created.",
            "examples": ["2026-02-04T10:30:00Z"]
          },
          "completed_at": {
            "type": ["null", "string"],
            "format": "date-time",
            "readOnly": true,
            "description": "Timestamp when the process completed execution. Null if the process has never completed or is currently running.",
            "examples": ["2026-02-04T10:35:45Z"]
          },
          "save": {
            "type": "boolean",
            "default": false
          },
          "lint": {
            "type": "boolean",
            "default": false
          },
          "run": {
            "type": "boolean",
            "default": true
          }
        }
      },
      "Definition": {
        "type": "object",
        "required": ["id", "name", "type", "specification_file"],
        "properties": {
          "id": {
            "type": "string",
            "pattern": "^[a-zA-Z0-9-_]+$",
            "description": "Unique slug for the definition.",
            "examples": ["model-control-v1_stable"]
          },
          "name": {
            "type": "string",
            "description": "A human-readable name for the definition.",
            "examples": ["Data Processor v2"]
          },
          "description": {
            "type": "string",
            "description": "A detailed explanation of what this definition handles.",
            "examples": ["Standard ETL definition for processing CSV uploads."]
          },
          "type": {
            "type": "string",
            "description": "The format or category of the definition.",
            "examples": ["openapi"]
          },
          "specification_file": {
            "type": "string",
            "format": "binary",
            "description": "The actual file content of the specification file."
          },
          "source_url": {
            "type": "string",
            "format": "uri",
            "description": "Optional URL where this definition was fetched from (for registry-installed definitions).",
            "examples": [
              "https://registry.example.com/api/definitions/model-control-v1_stable"
            ]
          }
        }
      },
      "InstallRequest": {
        "type": "object",
        "required": ["url"],
        "properties": {
          "url": {
            "type": "string",
            "format": "uri",
            "description": "The direct URL to the API endpoint containing the definition/module metadata and files.",
            "examples": [
              "https://registry.example.com/api/definitions/model-control-v1_stable"
            ]
          }
        }
      },
      "Module": {
        "type": "object",
        "required": ["id", "name", "type", "wasm_file"],
        "properties": {
          "id": {
            "type": "string",
            "pattern": "^[a-zA-Z0-9-_]+$",
            "description": "Unique slug for the module.",
            "examples": ["docker_sandbox"]
          },
          "name": {
            "type": "string",
            "description": "A human-readable name for the module.",
            "examples": ["Docker Sandbox"]
          },
          "description": {
            "type": "string",
            "description": "A detailed explanation of what this module handles.",
            "examples": ["Docker based sandbox environment"]
          },
          "type": {
            "type": "string",
            "enum": ["sandbox", "language", "proxy", "interceptor", "hook"],
            "description": "The format or category of the module.",
            "examples": ["sandbox"]
          },
          "wasm_file": {
            "type": "string",
            "format": "binary",
            "description": "The actual file content of the wasm file."
          },
          "source_url": {
            "type": "string",
            "format": "uri",
            "description": "Optional URL where this module was fetched from (for registry-installed modules).",
            "examples": [
              "https://registry.example.com/api/modules/docker_sandbox"
            ]
          }
        }
      },
      "LogResponse": {
        "type": "object",
        "properties": {
          "stdout": {
            "type": "string"
          },
          "stderr": {
            "type": "string"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "Error": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string",
            "examples": ["Conflict"]
          },
          "message": {
            "type": "string",
            "examples": [
              "Cannot delete a process while it is in the 'running' state."
            ]
          },
          "details": {
            "type": "object"
          }
        }
      }
    }
  },
  "security": [
    {
      "mci_auth": [],
      "clientId": []
    },
    {
      "apiToken": [],
      "clientId": []
    }
  ],
  "paths": {
    "/processes": {
      "get": {
        "tags": ["process"],
        "summary": "List all processes",
        "description": "Retrieve a list of processes with optional filtering and pagination.",
        "parameters": [
          {
            "name": "query",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "Filter processes by query name and description"
          },
          {
            "name": "status",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["idle", "running", "paused"]
            },
            "description": "Filter processes by status"
          },
          {
            "name": "exit_state",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["success", "failed", "terminated"]
            },
            "description": "Filter processes by exit state"
          },
          {
            "name": "environment",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "Filter processes by environment"
          },
          {
            "name": "created_after",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "description": "Filter processes created after this timestamp",
            "example": "2026-02-01T00:00:00Z"
          },
          {
            "name": "created_before",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "description": "Filter processes created before this timestamp",
            "example": "2026-02-04T23:59:59Z"
          },
          {
            "name": "completed_after",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "description": "Filter processes completed after this timestamp"
          },
          {
            "name": "completed_before",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "description": "Filter processes completed before this timestamp"
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 50,
              "minimum": 1,
              "maximum": 100
            },
            "description": "Maximum number of results to return"
          },
          {
            "name": "offset",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 0,
              "minimum": 0
            },
            "description": "Number of results to skip (for pagination)"
          },
          {
            "name": "sort_by",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["created_at", "completed_at", "name", "pid"],
              "default": "created_at"
            },
            "description": "Field to sort by"
          },
          {
            "name": "sort_order",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["asc", "desc"],
              "default": "desc"
            },
            "description": "Sort order (ascending or descending)"
          }
        ],
        "responses": {
          "200": {
            "description": "A list of processes.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Process"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": ["process"],
        "summary": "Create/Run a process",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Process"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Process created/initiated.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Process"
                }
              }
            }
          },
          "422": {
            "description": "Linting failure.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/processes/{pid}": {
      "get": {
        "tags": ["process"],
        "summary": "Get process details",
        "parameters": [
          {
            "name": "pid",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Process state.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Process"
                }
              }
            }
          },
          "404": {
            "description": "Not found."
          }
        }
      },
      "put": {
        "tags": ["process"],
        "summary": "Modify process",
        "parameters": [
          {
            "name": "pid",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Process"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Updated."
          },
          "409": {
            "description": "Conflict: Cannot modify running process.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": ["process"],
        "summary": "Delete process",
        "parameters": [
          {
            "name": "pid",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Deleted successfully."
          },
          "409": {
            "description": "Conflict: Cannot delete running process.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/processes/{pid}/logs": {
      "get": {
        "tags": ["process"],
        "summary": "Get process logs",
        "parameters": [
          {
            "name": "pid",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LogResponse"
                }
              }
            }
          }
        }
      }
    },
    "/processes/{pid}/output": {
      "get": {
        "tags": ["process"],
        "summary": "Get process output",
        "description": "Retrieve the output of a process (not execution logs). This endpoint may support streaming in future versions.",
        "parameters": [
          {
            "name": "pid",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            },
            "description": "The process ID"
          }
        ],
        "responses": {
          "200": {
            "description": "Process output as plain text.",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "404": {
            "description": "Process not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/processes/{pid}/signal": {
      "post": {
        "tags": ["process"],
        "summary": "Send a signal to a process",
        "parameters": [
          {
            "name": "pid",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["signal"],
                "properties": {
                  "signal": {
                    "type": "string",
                    "enum": ["kill", "pause", "restart"]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Signal sent successfully."
          }
        }
      }
    },
    "/processes/{pid}/start": {
      "post": {
        "tags": ["process"],
        "summary": "Start idle process",
        "parameters": [
          {
            "name": "pid",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Started successfully."
          }
        }
      }
    },
    "/definitions": {
      "get": {
        "tags": ["definition"],
        "summary": "List all definitions",
        "responses": {
          "200": {
            "description": "A list of definitions.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Definition"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": ["definition"],
        "summary": "Create a new definition",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Definition"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Definition created successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Definition"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "409": {
            "description": "Definition with this ID already exists.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/definitions/{id}": {
      "get": {
        "tags": ["definition"],
        "summary": "Get definition metadata and specification file",
        "description": "Retrieves the complete definition including metadata and the specification file content.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The definition ID"
          }
        ],
        "responses": {
          "200": {
            "description": "Definition data including metadata and specification file.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Definition"
                }
              }
            }
          },
          "404": {
            "description": "Definition not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": ["definition"],
        "summary": "Update an existing definition",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The definition ID"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Definition"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Definition updated successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Definition"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "404": {
            "description": "Definition not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": ["definition"],
        "summary": "Delete a definition",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The definition ID"
          }
        ],
        "responses": {
          "204": {
            "description": "Definition deleted successfully."
          },
          "404": {
            "description": "Definition not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "409": {
            "description": "Cannot delete definition (e.g., in use by processes).",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/definitions/{id}/update": {
      "post": {
        "tags": ["definition"],
        "summary": "Update definition from source URL",
        "description": "Updates the definition by fetching the latest version from its source_url.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The definition ID"
          }
        ],
        "responses": {
          "200": {
            "description": "Definition updated successfully from source.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Definition"
                }
              }
            }
          },
          "400": {
            "description": "Definition does not have a source_url.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "404": {
            "description": "Definition not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "502": {
            "description": "Failed to fetch from source URL.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/definitions/install": {
      "post": {
        "tags": ["definition"],
        "summary": "Install a definition from a URL",
        "description": "Automatically downloads, validates, and installs a definition from a specified URL.",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InstallRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Definition installed successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Definition"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request or validation failure.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "409": {
            "description": "Definition already exists.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "502": {
            "description": "Failed to fetch from URL.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/modules": {
      "get": {
        "tags": ["module"],
        "summary": "List all modules",
        "responses": {
          "200": {
            "description": "A list of modules.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Module"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": ["module"],
        "summary": "Create a new module",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Module"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Module created successfully.",
            "content": {
              "application/json": {
